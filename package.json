{
	"name": "not-meta",
	"version": "0.0.24",
	"description": "not-* family meta layer for model and route",
	"main": "index.js",
	"engines": {
		"node": ">=12.0"
	},
	"scripts": {
		"test": "./node_modules/.bin/mocha --reporter spec",
		"pretest": "./node_modules/.bin/eslint ./src/**/**",
		"lint": "./node_modules/.bin/eslint ./src/**/**",
		"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"cover": "istanbul cover _mocha $(find ./test -name \"*.js\" -not -path \"./node_modules/*\")"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-meta.git"
	},
	"keywords": [
		"not-framework",
		"not-node"
	],
	"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-meta/issues"
	},
	"homepage": "https://github.com/interrupter/not-meta#readme",
	"dependencies": {
		"not-config": "*",
		"not-node": "*",
		"not-error": "*",
		"not-filter": "*",
		"not-log": "*"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.16.3",
    "babel-eslint": "^10.1.0",
		"eslint": "^7.0.0",
		"eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-sonarjs": "^0.11.0",
		"chai": "*",
		"chai-as-promised": "*",
		"ink-docstrap": "^1.3.2",
		"istanbul": "*",
		"jsdoc": "^3.6.7",
		"mocha": "*"
	}
}
